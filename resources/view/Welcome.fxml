<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" styleClass="anchorpane" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
      <Label fx:id="lblNoSelect" alignment="CENTER" layoutX="139.0" layoutY="575.0" prefHeight="21.0" prefWidth="540.0" style="-fx-text-fill: #ff5050;" textAlignment="CENTER" textFill="#e11010">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <ImageView fitHeight="68.0" fitWidth="350.0" layoutX="300.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/titulo.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="161.0" fitWidth="161.0" layoutX="135.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/logo_min.png" />
         </image>
      </ImageView>
		<Label layoutX="310.0" layoutY="75.0" text="%label.welcome">
			<font>
				<Font size="22.0" />
			</font>
		</Label>
		<Label fx:id="lblUser" layoutX="450.0" layoutY="75.0" text="Nombre de usuario">
			<font>
				<Font size="22.0" />
			</font>
		</Label>
      <AnchorPane fx:id="anchorPane" layoutX="75.0" layoutY="175.0" prefHeight="350.0" prefWidth="650.0">
         <children>
            <TabPane fx:id="tabPane" prefHeight="290.0" prefWidth="650.0" styleClass="tab-pane-class" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                  <Tab text="%text.recent">
                     <content>
                        <ListView fx:id="listCoursesRecent" prefHeight="250.0" prefWidth="650.0" styleClass="list-view-intro" stylesheets="@/css/style.css">
       						<placeholder>
                  				<Label text="%label.never" />
              				</placeholder>
                           <cursor>
                              <Cursor fx:constant="DEFAULT" />
                           </cursor>
                        </ListView>
                     </content>
                  </Tab>
                <Tab text="%text.favorite">
                     <content>
                        <ListView fx:id="listCoursesFavorite" prefHeight="250.0" prefWidth="650.0" styleClass="list-view-intro" stylesheets="@/css/style.css">
      						<placeholder>
                  				<Label text="%label.never" />
              				</placeholder>
                           <cursor>
                              <Cursor fx:constant="DEFAULT" />
                           </cursor>
                        </ListView>
                     </content>
                  </Tab>
                <Tab text="%text.all">
                     <content>
                  		<ListView fx:id="listCourses" styleClass="list-view-intro" stylesheets="@/css/style.css">
      						<placeholder>
                  				<Label text="%label.never" />
              				</placeholder>
                           <cursor>
                              <Cursor fx:constant="DEFAULT" />
                           </cursor>
                        </ListView>
                     </content>
                  </Tab>
                  <Tab text="%text.inprogress">
                     <content>
                        <ListView fx:id="listCoursesInProgress" styleClass="list-view-intro" stylesheets="@/css/style.css">
                           <cursor>
                              <Cursor fx:constant="DEFAULT" />
                           </cursor>
                           <placeholder>
                              <Label text="%label.never" />
                           </placeholder>
                        </ListView>
                     </content>
                  </Tab>
                  <Tab text="%text.future">
                     <content>
                        <ListView fx:id="listCoursesFuture" styleClass="list-view-intro" stylesheets="@/css/style.css">
                           <cursor>
                              <Cursor fx:constant="DEFAULT" />
                           </cursor>
                           <placeholder>
                              <Label text="%label.never" />
                           </placeholder>
                        </ListView>
                     </content>
                  </Tab>
                  <Tab text="%text.past">
                     <content>
                        <ListView fx:id="listCoursesPast" styleClass="list-view-intro" stylesheets="@/css/style.css">
                           <cursor>
                              <Cursor fx:constant="DEFAULT" />
                           </cursor>
                           <placeholder>
                              <Label text="%label.never" />
                           </placeholder>
                        </ListView>
                     </content>
                  </Tab>
              </tabs>
            </TabPane>
            <Label layoutX="70.0" layoutY="300.0" prefHeight="21.0" prefWidth="258.0" text="%label.lastupdate" />
            <CheckBox fx:id="chkUpdateData" layoutX="70.0" layoutY="330.0" mnemonicParsing="false" text="%checkbox.updatedata" />
            <Label fx:id="lblDateUpdate" layoutX="300.0" layoutY="300.0" prefHeight="21.0" prefWidth="146.0" />
         </children>
      </AnchorPane>
		<Label layoutX="310.0" layoutY="145.0" text="%label.selectcourse">
			<font>
				<Font size="15.0" />
			</font>
		</Label>
		<Button fx:id="btnEntrar" layoutX="368.0" layoutY="530.0" mnemonicParsing="false" onAction="#enterCourse" prefHeight="39.0" prefWidth="86.0" style="-fx-font-size: 16;" styleClass="rich-blue" stylesheets="@/css/style.css" text="%btn.enter">
			<font>
				<Font size="19.0" />
			</font>
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Button>
      <ProgressBar fx:id="progressBar" layoutX="340.0" layoutY="540.0" prefHeight="18.0" prefWidth="139.0" />
      <Label fx:id="lblProgress" alignment="CENTER" layoutX="136.0" layoutY="575.0" prefHeight="17.0" prefWidth="540.0" text="Progress" textAlignment="CENTER" visible="false">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
	</children>
</AnchorPane>
